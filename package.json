{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Main portfolio at https://saidelimam.com",
  "main": "index.js",
  "scripts": {
    "build-css": "npx lessc src/styles/main.less src/styles/main.css",
    "watch-css": "npx less-watch-compiler src/styles src/styles main.less",
    "dev": "vite",
    "build": "vitest run && vite build",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.{html,css,less,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{html,css,less,js,json,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saidelimam/portfolio.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/saidelimam/portfolio/issues"
  },
  "homepage": "https://github.com/saidelimam/portfolio#readme",
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/user-event": "^14.6.1",
    "@vitest/ui": "^4.0.6",
    "concurrently": "^9.2.1",
    "jsdom": "^27.1.0",
    "less": "^4.2.0",
    "less-watch-compiler": "^1.16.3",
    "prettier": "^3.2.5",
    "terser": "^5.44.0",
    "vite": "^7.1.12",
    "vitest": "^4.0.6"
  }
}
